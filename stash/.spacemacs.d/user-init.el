;; Loaded by "init.el".

;; Place for any initialization of user code to be executed before
;; layers/packages are loaded.
(defun dotspacemacs/user-init ()

  "Initialization function for user code."

  ;; Make comments less ugly
  (setq spacemacs-theme-comment-bg nil)
  (setq spacemacs-theme-comment-italic f)

  )
